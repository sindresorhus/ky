	"devDependencies": {
		"@types/jquery": "^3.3.22",
		"ava": "1.0.0-beta.8",
		"body": "^5.1.0",
		"codecov": "^3.0.4",
		"create-test-server": "2.1.1",
		"cypress": "^3.1.0",
		"delay": "^4.0.0",
		"esm": "^3.0.82",
		"node-fetch": "^2.2.0",
